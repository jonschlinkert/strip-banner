{
  "name": "strip-banner",
  "description": "Strip banners from code.",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/strip-banner",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/strip-banner",
  "bugs": {
    "url": "https://github.com/jonschlinkert/strip-banner/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "dependencies": {
    "extract-banner": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "banner",
    "banners",
    "comment",
    "comments",
    "remove",
    "strip"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "has-banner",
        "extract-banner",
        "add-banner",
        "write-banner",
        "update-banner"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}